{
  "name": "zOS",
  "version": "0.2.9",
  "private": true,
  "main": "./public/electron.js",
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@reduxjs/toolkit": "^1.7.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.20.37",
    "@types/react": "^16.14.21",
    "@types/react-dom": "^16.9.14",
    "@types/react-redux": "^7.1.20",
    "@web3-react/core": "^6.1.9",
    "@web3-react/fortmatic-connector": "^6.1.6",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/network-connector": "^6.1.9",
    "@web3-react/portis-connector": "^6.2.11",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "@web3-react/walletlink-connector": "^6.2.13",
    "@zer0-os/zos-component-library": "0.10.0",
    "@zer0-os/zos-feed": "1.20.0",
    "@zer0-os/zos-zns": "2.2.0",
    "classnames": "^2.3.1",
    "es6-promise-debounce": "^1.0.1",
    "ethers": "^5.5.2",
    "global": "^4.4.0",
    "history": "^4.10.1",
    "lodash.get": "^4.4.2",
    "lodash.kebabcase": "^4.1.1",
    "normalizr": "^3.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-portal": "^4.2.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "sass": "^1.49.7",
    "superagent": "^6.1.0",
    "typescript": "^4.1.6"
  },
  "scripts": {
    "start": "craco start",
    "lint": "eslint src",
    "build": "craco build",
    "test": "craco test --env=node",
    "eject": "react-scripts eject",
    "electron:start": "concurrently -k \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron:package:publish": "craco build && electron-builder -m -w -c.extraMetadata.main=build/electron.js --config electron/electron-builder-config.js --publish=always",
    "electron:package:mac": "craco build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "craco build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "craco build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "code-format:staged:fix": "pretty-quick --staged",
    "code-format:validate": "prettier --check 'src/**/*.{ts,tsx,json,md,js,yml,html,scss}'",
    "code-format:fix": "prettier --write 'src/**/*.{ts,tsx,json,md,js,yml,html,scss}'",
    "prepare": "husky install"
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.10",
    "@types/react-portal": "^4.0.4",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "electronmon": "^2.0.2",
    "enzyme": "^3.11.0",
    "husky": "^8.0.1",
    "jest-extended": "^1.2.0",
    "prettier": "^2.7.1",
    "prettier-plugin-multiline-arrays": "^1.0.0",
    "pretty-quick": "^3.1.3",
    "redux-saga-test-plan": "^4.0.4",
    "wait-on": "^6.0.1"
  }
}
